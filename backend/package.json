{
  "name": "backend",
  "version": "1.0.1",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
    "build": "tsc && cpx \"src/templates/**/*\" dist/templates && cpx \"src/.env\" dist/ --verbose && cpx src/userStatus.json dist/",
    "obfuscate": "javascript-obfuscator dist --output dist --config obfuscator-config.json",
    "build:obfuscate": "npm run build && npm run obfuscate",
    "generate-mappings": "ts-node -r tsconfig-paths/register scripts/generateMappings.ts"
  },
